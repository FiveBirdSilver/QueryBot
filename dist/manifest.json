{
  "manifest_version": 3,
  "name": "GenAIon",
  "version": "1.0",
  "description": "A Chrome Extension with GA4 Chatbot.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "logo.png"
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "permissions": ["sidePanel", "storage"],
  "content_scripts": [
    {
      "matches": ["*://velog.io/*"],
      "js": ["content.js"]
    }
  ]
}
